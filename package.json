{
  "name": "@trafficguard/nous",
  "version": "0.0.1-dev",
  "description": "AI agent & LLM app platform",
  "private": true,
  "engines": {
    "node": ">=20.6.0"
  },
  "scripts": {
    "clean": "echo dumy clean",
    "chat": "    node --env-file=variables/local.env -r ts-node/register -r tsconfig-paths/register --inspect=0.0.0.0:9229 src/cli/chat.ts",
    "agent": "   node --env-file=variables/local.env -r ts-node/register -r tsconfig-paths/register --inspect=0.0.0.0:9229 src/cli/agent.ts",
    "bench": "   node --env-file=variables/local.env -r ts-node/register -r tsconfig-paths/register --inspect=0.0.0.0:9229 src/cli/bench.ts",
    "pyagent": " node --env-file=variables/local.env -r ts-node/register -r tsconfig-paths/register --inspect=0.0.0.0:9229 src/cli/pyAgent.ts",
    "code": "    node --env-file=variables/local.env -r ts-node/register -r tsconfig-paths/register --inspect=0.0.0.0:9229 src/cli/code.ts",
    "swe": "     node --env-file=variables/local.env -r ts-node/register -r tsconfig-paths/register --inspect=0.0.0.0:9229 src/cli/swe.ts",
    "scrape": "  node --env-file=variables/local.env -r ts-node/register -r tsconfig-paths/register --inspect=0.0.0.0:9229 src/cli/scrape.ts",
    "research": "node --env-file=variables/local.env -r ts-node/register -r tsconfig-paths/register --inspect=0.0.0.0:9229 src/cli/research.ts",
    "util": "    node --env-file=variables/local.env -r ts-node/register -r tsconfig-paths/register --inspect=0.0.0.0:9229 src/cli/util.ts",
    "build": "    tsc",
    "build:dev": "tsc -project tsconfig.swc.json",
    "functions": "node --env-file=variables/local.env -r ts-node/register -r tsconfig-paths/register src/generateToolDefinitions.ts",
    "start": "      node -r ts-node/register -r tsconfig-paths/register src/index.ts",
    "start:local": "node -r ts-node/register -r tsconfig-paths/register --env-file=variables/local.env src/index.ts",
    "start:file": " node -r ts-node/register -r tsconfig-paths/register --env-file=variables/local.env src/index.ts  --db=file",
    "test": "npm run test:unit && echo \"No system or integration tests\"",
    "test:unit": "       node --env-file=variables/test.env   ./node_modules/mocha/bin/mocha -r ts-node/register -r tsconfig-paths/register \"src/**/*.test.[jt]s\" --timeout 10000",
    "test:integration": "node --env-file=variables/test.env    ./node_modules/mocha/bin/mocha -r ts-node/register -r tsconfig-paths/register \"src/**/*.int.[jt]s\" --timeout 0  --exit",
    "test:system": "     node --env-file=variables/test.env ./node_modules/mocha/bin/mocha -r ts-node/register -r tsconfig-paths/register \"src/**/*.sys.ts\" --timeout 0  --exit",
    "test:unit:dev": "TS_NODE_PROJECT='./tsconfig.swc.json' node  --env-file=variables/test.env ./node_modules/mocha/bin/mocha -r ts-node/register -r tsconfig-paths/register -t 0 --exit --colors \"src/**/*.test.[jt]s\"",
    "format": "biome format --write ./src",
    "lint": "  biome check --apply-unsafe ./src",
    "lint:ci": "biome ci ./src",
    "prepare": "node .husky/install.mjs"
  },
  "keywords": [],
  "author": "daniel.campagnoli@trafficguard.ai",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.2",
    "@anthropic-ai/vertex-sdk": "^0.4.0",
    "@fastify/cors": "^9.0.1",
