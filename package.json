    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "chai": "^4.4.1",
    "chai-subset": "^1.6.0",
    "drizzle-kit": "^0.20.14",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mocha": "^10.3.0",
    "npm-run-all": "^4.1.5",
    "secretlint": "^8.2.4",
    "sinon": "^17.0.0",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^4.0.0",
