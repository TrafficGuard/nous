    "@google-cloud/vertexai": "^1.1.0",
    "@grpc/grpc-js": "^1.8.0",
    "@mozilla/readability": "^0.5.0",
    "@octokit/request": "^5.1.0",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/auto-instrumentations-node": "^0.44.0",
    "@opentelemetry/instrumentation": "^0.50.0",
