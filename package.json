    "http-status-codes": "^2.3.0",
    "ignore": "^5.3.1",
    "jsdom": "^24.0.0",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "openai": "^4.28.4",
    "pino": "^8.18.0",
    "pino-pretty": "^11.0.0",
    "playwright": "^1.42.1",
    "puppeteer": "^22.6.5",
    "pyodide": "^0.26.1",
    "readline": "^1.3.0",
    "serpapi": "^2.1.0",
    "ts-morph": "^21.0.1",
    "ts-node": "^10.7.0",
